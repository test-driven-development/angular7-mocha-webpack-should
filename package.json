{
  "name": "angular7-webpack-mocha-should",
  "version": "1.0.0",
  "description": "tdd workflow for angular 7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/test-driven-development/angular7-mocha-webpack-should"
  },
  "keywords": [
    "angular",
    "tdd",
    "mocha",
    "webpack",
    "mocha-loader",
    "mocha-webpack",
    "jsdom",
    "node"
  ],
  "author": "Wil Pannell (h/t: Hicham BOUZIDI IDRISSI)",
  "bugs": {
    "url": "https://github.com/test-driven-development/angular7-mocha-webpack-should/issues"
  },
  "homepage": "hthttps://github.com/test-driven-development/angular7-mocha-webpack-should#readme",
  "scripts": {
    "start": "webpack-dev-server --mode development --watch-content-base",
    "test": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:watch": "rimraf .tmp && mocha-webpack --opts config/mocha/mocha-webpack.opts --watch",
    "test:server": "webpack-dev-server --mode development --config config/webpack.test.browser.js",
    "test:coverage": "rimraf reports && cross-env NODE_ENV=coverage nyc --reporter=text --reporter=text-summary mocha-webpack --opts config/mocha/mocha-webpack.opts",
    "test:reports": "rimraf reports && cross-env NODE_ENV=coverage nyc mocha-webpack --opts config/mocha/mocha-webpack.opts --reporter=xunit --reporter-options output=reports/tests/tests-report.xml",
    "tslint": "tslint --format stylish --project tsconfig.json -c tslint.json --force"
  },
  "dependencies": {
    "@angular/animations": "7.1.0",
    "@angular/common": "7.1.0",
    "@angular/compiler": "7.1.0",
    "@angular/core": "7.1.0",
    "@angular/forms": "7.1.0",
    "@angular/http": "7.1.0",
    "@angular/platform-browser": "7.1.0",
    "@angular/platform-browser-dynamic": "7.1.0",
    "@angular/platform-server": "7.1.0",
    "@angular/router": "7.1.0",
    "bootstrap": "4.1.3",
    "core-js": "2.5.4",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "popper.js": "1.14.6",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.0",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.1.0",
    "@angular/language-service": "7.1.0",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.15",
    "angular2-template-loader": "0.6.2",
    "atob": "2.1.0",
    "awesome-typescript-loader": "5.2.1",
    "codelyzer": "4.5.0",
    "cross-env": "5.2.0",
    "css-loader": "2.0.1",
    "css-to-string-loader": "0.1.3",
    "file-loader": "2.0.0",
    "file-saver": "2.0.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jsdom": "13.0.0",
    "keycloak-angular": "6.0.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.4.4",
    "mixin-deep": "2.0.0",
    "mocha": "5.2.0",
    "mocha-loader": "2.0.0",
    "mocha-webpack": "2.0.0-beta.0",
    "moment": "2.23.0",
    "null-loader": "0.1.1",
    "nyc": "13.1.0",
    "ramda": "0.26.1",
    "ramda-fantasy": "0.8.0",
    "rimraf": "2.6.2",
    "should": "13.2.3",
    "sinon": "7.1.1",
    "style-loader": "0.23.1",
    "testdouble": "3.9.1",
    "timekeeper": "2.1.2",
    "to-string-loader": "1.1.5",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "webpack": "4.26.0",
    "webpack-archive-plugin": "3.0.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-middleware": "3.4.0",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "text",
      "text-summary",
      "html"
    ],
    "instrument": false,
    "sourceMap": false,
    "report-dir": "./reports/coverage"
  }
}
